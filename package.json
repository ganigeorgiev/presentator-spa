{
  "name": "presentator-spa",
  "description": "Presentator v2 SPA",
  "private": true,
  "author": "Gani Georgiev <gani.georgiev@gmail.com>",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git://github.com/presentator/presentator-spa.git"
  },
  "scripts": {
